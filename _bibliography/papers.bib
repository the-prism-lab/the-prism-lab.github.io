@article{williams_secondary_2024,
	title = {Secondary {Amine} {Catalysis} in {Enzyme} {Design}: {Broadening} {Protein} {Template} {Diversity} through {Genetic} {Code} {Expansion}},
	volume = {63},
	issn = {1433-7851, 1521-3773},
	shorttitle = {Secondary {Amine} {Catalysis} in {Enzyme} {Design}},
	url = {https://onlinelibrary.wiley.com/doi/10.1002/anie.202403098},
	doi = {10.1002/anie.202403098},
	abstract = {Secondary amines, due to their reactivity, can transform protein templates into catalytically active entities, accelerating the development of artificial enzymes. However, existing methods, predominantly reliant on modified ligands or N-terminal prolines, impose significant limitations on template selection. In this study, genetic code expansion was used to break this boundary, enabling secondary amines to be incorporated into alternative proteins and positions of choice. Pyrrolysine analogues carrying different secondary amines could be incorporated into superfolder green fluorescent protein (sfGFP), multidrug-binding LmrR and nucleotide-binding dihydrofolate reductase (DHFR). Notably, the analogue containing a D-proline moiety demonstrated both proteolytic stability and catalytic activity, conferring LmrR and DHFR with the desired transfer hydrogenation activity. While the LmrR variants were confined to the biomimetic 1-benzyl-1,4-dihydronicotinamide (BNAH) as the hydride source, the optimal DHFR variant favorably used the pro-R hydride from NADPH for stereoselective reactions (e.r. up to 92 : 8), highlighting that a switch of protein template could broaden the nucleophile option for catalysis. Owing to the cofactor compatibility, the DHFR-based secondary amine catalysis could be integrated into an enzymatic recycling scheme. This established method shows substantial potential in enzyme design, applicable from studies on enzyme evolution to the development of new biocatalysts.},
	language = {en},
	number = {22},
	urldate = {2024-07-17},
	journal = {Angew. Chem. Int. Ed.},
	author = {Williams, Thomas L. and Taily, Irshad M. and Hatton, Lewis and Berezin, Andrey A and Wu, Yi‐Lin and Moliner, Vicent and Świderek, Katarzyna and Tsai, Yu‐Hsuan and Luk, Louis Y. P.},
	month = may,
	year = {2024},
	pages = {e202403098},
}

@article{chen_selective_2024,
	title = {Selective {Protein} {Degradation} through {Tetrazine} {Ligation} of {Genetically} {Incorporated} {Unnatural} {Amino} {Acids}},
	issn = {1861-4728, 1861-471X},
	url = {https://aces.onlinelibrary.wiley.com/doi/10.1002/asia.202400824},
	doi = {10.1002/asia.202400824},
	abstract = {Small molecule‐responsive tags for targeted protein degradation are valuable tools for fundamental research and drug target validation. Here, we show that genetically incorporated unnatural amino acids bearing a strained alkene or alkyne functionality can act as a minimalist tag for targeted protein degradation. Specifically, we observed the degradation of strained alkene‐ or alkyne‐containing kinases and E2 ubiquitin‐conjugating enzymes upon treatment with hydrophobic tetrazine conjugates. The extent of the induced protein degradation depends on the identity of the target protein, unnatural amino acid, and tetrazine conjugate, as well as the site of the unnatural amino acid in the target protein. Mechanistic studies revealed proteins undergo proteasomal degradation after tetrazine tethering, and the identity of tetrazine conjugates influences the dependence of ubiquitination on protein degradation. This work provides an alternative approach for targeted protein degradation and mechanistic insight, facilitating the future development of more effective targeted protein degradation strategies.},
	language = {en},
	urldate = {2024-11-19},
	journal = {Chem. Asian J.},
	author = {Chen, Jinghao and Dai, Gaocan and Duan, Shixiang and Huang, Yang and Wu, Yi‐Lin and Xie, Zhiyong and Tsai, Yu‐Hsuan},
	month = oct,
	year = {2024},
	keywords = {notion},
	pages = {e202400824},
}

@article{huang_single_2025,
	title = {A Single Bioorthogonal Reaction for Multiplex Cell Surface Protein Labeling},
	volume = {147},
	copyright = {https://creativecommons.org/licenses/by/4.0/},
	issn = {0002-7863, 1520-5126},
	url = {https://pubs.acs.org/doi/10.1021/jacs.4c11701},
	doi = {10.1021/jacs.4c11701},
	abstract = {Small-molecule fluorophores are invaluable tools for fluorescence imaging. However, means for their covalent conjugation to the target proteins limit applications in multicolor imaging. Here, we identify 2-[(alkylthio)(aryl)methylene]malononitrile (TAMM) molecules reacting with 1,2-aminothiol at a labeling rate over 104 M–1 s–1 through detailed mechanistic investigation. The fast TAMM molecules and mild reaction conditions enable site-specific labeling of surface proteins in not only cell lines but also primary neurons and living mice. The combination of genetic code expansion and sequence-specific proteolytic cleavage enables selective modification of three different cell surface proteins through iterative TAMM condensation. TAMM condensation is also compatible with Cu-catalyzed azide–alkyne cycloaddition and tetrazine ligation for four-color fluorescent labeling, reaching the maximum available colors of conventional confocal microscopes. Thus, bioconjugation chemistry is no longer the limiting factor for multiplex cell surface protein imaging.},
	language = {en},
	number = {2},
	urldate = {2025-06-24},
	journal = {J. Am. Chem. Soc.},
	author = {Huang, Yang and Wu, Chengyang and Lu, Anjing and Wang, Jingzhe and Liang, Jian and Sun, Han and Yang, Liqing and Duan, Shixiang and Berezin, Andrey A. and Wu, Chuanliu and Zhang, Bo and Wu, Yi-Lin and Tsai, Yu-Hsuan},
	month = jan,
	year = {2025},
	pages = {1612--1623},
	selected={true},
}
