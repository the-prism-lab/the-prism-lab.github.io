	pages = {1342--1348},
	selected={true},
	file = {Wu_Wright_2023_Why does thionating a carbonyl molecule make it a better electron acceptor.pdf:C\:\\Users\\carbon\\OneDrive - Cardiff University\\Zotero_YLW\\Wu_Wright_2023_Why does thionating a carbonyl molecule make it a better electron acceptor.pdf:application/pdf},
}

